        "ts-loader": "^6.0.1",
        "tslint": "^5.16.0",
        "typescript": "^3.6.2",
        "webpack": "^4.31.0",
        "webpack-cli": "^3.3.2",
        "webpack-dev-server": "^3.4.1"
    },
